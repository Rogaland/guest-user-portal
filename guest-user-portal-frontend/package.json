{
  "name": "guest-user-portal-frontend",
  "version": "0.0.0",
  "main": "main.js",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "test": "ng test",
    "build": "ng build --prod --progress=false",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build-electron": "npm run ng build --base-href . && copy electron\\* dist",
    "electron:local": "npm run build-electron && electron dist\\main.js http://localhost:8080",
    "electron": "npm run build-electron && electron dist\\main.js",
    "electron:package": "npm run build-electron && copy package.json dist && electron-packager dist --overwrite true --out releases"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "electron": "^1.4.15",
    "font-awesome": "^4.7.0",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-beta.32.3",
    "@types/electron": "^1.4.32",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.1",
    "electron-builder": "^10.9.2",
    "electron-packager": "^8.1.0",
    "electron-prebuilt": "^1.3.10",
    "ember-electron": "^1.12.6",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "ncp": "^2.0.0",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  }
}
